package com.herongtech.console.domain.common.bean;

import java.io.Serializable;

public class ExportInfoBean implements Serializable{
	public static final String HERONGTECH_VERSION="@system 票据管理平台 @version 2.0.0.1 @lastModiDate 20161015 @describe ";
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private String fieldName;//字段
	private String exFieldName;//字段名
	private String tableName;//表明
	private String exTableName;//导出后excel名称
	private String tableAlias;//表别名
	private String isExport;//是否导出
	private String operTime;//导出时间
	private String operUser;//导出人
	private String isTotal;//是否汇总 1汇总 0不汇总
	private String exOrder;//列序
	private String exType;//导出类型
	private String exDict;//对应数据字典
	private String exAlign;//对齐方式 1左对齐 2居中  3右对齐
	private String exCondition;//查询条件
	private String conditionValue;//条件值
	
	public String getConditionValue() {
		return conditionValue;
	}
	public void setConditionValue(String conditionValue) {
		this.conditionValue = conditionValue;
	}
	public String getExCondition() {
		return exCondition;
	}
	public void setExCondition(String exCondition) {
		this.exCondition = exCondition;
	}
	public String getFieldName() {
		return fieldName;
	}
	public void setFieldName(String fieldName) {
		this.fieldName = fieldName;
	}
	public String getExFieldName() {
		return exFieldName;
	}
	public void setExFieldName(String exFieldName) {
		this.exFieldName = exFieldName;
	}
	public String getTableName() {
		return tableName;
	}
	public void setTableName(String tableName) {
		this.tableName = tableName;
	}
	public String getExTableName() {
		return exTableName;
	}
	public void setExTableName(String exTableName) {
		this.exTableName = exTableName;
	}
	public String getTableAlias() {
		return tableAlias;
	}
