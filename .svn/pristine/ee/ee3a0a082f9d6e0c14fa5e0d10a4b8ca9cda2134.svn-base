package com.herongtech.console.core.common.pubinfo;

public class ChannelFieldMetaData {
	/**
	 * 字符串
	 */
	public static final char TYPE_STR='S';
	/**
	 * 整形
	 */
	public static final char TYPE_INT='I';
	/**
	 * 浮点型
	 */
	public static final char TYPE_FLOAT='F';

	
	private String innerTagName=null;
	private String outerTagName=null;
	private int length=0;
	private int scale=0;
	private char type;
	
	public String getInnerTagName() {
		return innerTagName;
	}
	public void setInnerTagName(String innerTagName) {
		this.innerTagName = innerTagName;
	}
	public String getOuterTagName() {
		return outerTagName;
	}
	public void setOuterTagName(String outerTagName) {
		this.outerTagName = outerTagName;
	}
	public int getLength() {
		return length;
	}
	public void setLength(int length) {
		this.length = length;
	}
	public int getScale() {
		return scale;
	}
	public void setScale(int scale) {
		this.scale = scale;
	}
	public char getType() {
		return type;
	}
	public void setType(char type) {
		this.type = type;
	}
}
