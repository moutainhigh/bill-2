body{font-family:"微软雅黑","Microsoft YaHei";}
a,input,button{font-family:"微软雅黑","Microsoft YaHei";}
#jump-content label{font-family:"微软雅黑","Microsoft YaHei";}
#page-content label{font-family:"微软雅黑","Microsoft YaHei";}
/* tab标题工作台 */
#jerichotab_0 .tab_left div:first-child{
	font-family:"Microsoft YaHei";text-align:center;padding-left:10px;line-height:29px;text-indent:6px;width:75px;font-size:15px;
}
/* header背景图*/
#page-content #header{
	background:url(../assets/images/gallery/corners-sprite.gif) repeat-x top left !important;min-height:40px;height:auto !important;padding:6px 0 3px 0;;margin-bottom:3px;
	border: 1px solid #cfe2f0 !important;
}
/* 搜索结果 */
#sidebar #search_form #search_notice{
	display:none;width:157px!important;margin:-1px 0 0 0px!important;
}
/* 登录按钮*/
#loginbox #form-actions a{
		background:url(../assets/images/gallery/navbg.jpg) repeat-x !important;
		position:relative; z-index:9999; border-radius:5px 0px 0px 5px !important;behavior: url(ie-css3.htc);
}
#loginbox #form-actions a:hover{
		background:url(../assets/images/gallery/tab4.jpg) repeat-x !important; text-decoration:none;cursor:pointer;
}
.bg_lg{ 
	background:url(../assets/images/gallery/navbg.jpg) repeat-x !important;position:relative; z-index:9999; border-radius:5px 0px 0px 5px !important;behavior: url(ie-css3.htc);
}
/* 表格部分 */
.fixed-table-container .fixed-table-bg{
	background:rgb(240, 240, 248);
}
.fixed-table-container .fixed-table-header{
	margin-right:20px;
}
/* main.jsp四部分宽度 */
.welcom_dcl ul {
	width:47.5% !important;
}
/* 底部分页 */
.bottomPage{
	top:0px !important;line-height:35px !important;
}
/* 弹出框页面标题 */
#_Draghandle_0 td + td div{
	width:100px;
}
#_Draghandle_0 td + td div img{
	float:left;
}
#_Draghandle_0 td + td div span{
	float:left;
}
/* 票据详情页一行四个的显示 */
#page-content .detail-form .row-fluid label{
	width:90px;
}
#page-content #header .row-fluid .batch{
	width:177px;
}
/* fieldset标签 */
#jump-content .field{
	border:none;
}
/* jbox按钮 */
div.jbox .jbox-button{
	border:none;
}
/* jbox搜索 */
#tagSelectSearch{
	background:url(weblib/assets/images/gallery/navbg.jpg) repeat-x !important;border:none!important;padding:5px 4px!important;
	margin-left:-4px!important;color:#fff!important;font-size:14px!important;
}
/* 电票系统详细信息查看页面(正面背面信息) */
#ele-img{
	height:40px;margin-left:50px;
}