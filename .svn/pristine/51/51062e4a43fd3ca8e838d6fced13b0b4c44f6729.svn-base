package com.herongtech.rc.domain.bean;

import java.lang.*;
import java.util.*;
import java.math.*;

public class RgctEcdsStatus{
	/** default constructor */
	public RgctEcdsStatus() {
	}

	// Constructors

	public RgctEcdsStatus(String pname, String pvalue, String pnameCn, String pvalueCn) {
		super();
		this.pname = pname;
		this.pvalue = pvalue;
		this.pnameCn = pnameCn;
		this.pvalueCn = pvalueCn;
	}
	
	//名称
	private String pname = " ";
	public String getPname(){
		return pname;
	}
	public void setPname(String pname){
		this.pname = pname;
	}

	//名称中文
	private String pvalue = " ";
	public String getPvalue(){
		return pvalue;
	}
	public void setPvalue(String pvalue){
		this.pvalue = pvalue;
	}

	//码值
	private String pnameCn = " ";
	public String getPnameCn(){
		return pnameCn;
	}
	public void setPnameCn(String pnameCn){
		this.pnameCn = pnameCn;
	}

	//码值中文
	private String pvalueCn = " ";
	public String getPvalueCn(){
		return pvalueCn;
	}
	public void setPvalueCn(String pvalueCn){
		this.pvalueCn = pvalueCn;
	}


}
